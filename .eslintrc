{
  "extends": "airbnb",
  "env": {
    "es6": true,
    "jest": true,
    "browser": true
  },
  "rules": {
    "no-underscore-dangle": [
      "warn",
      {
        "allow": [
          "_id"
        ]
      }
    ],
    "jsx-a11y/anchor-is-valid": [
      "warn",
      {
        "components": [
          "Link"
        ],
        "specialLink": [
          "hrefLeft",
          "hrefRight"
        ],
        "aspects": [
          "invalidHref",
          "preferButton"
        ]
      }
    ],
    "jsx-a11y/click-events-have-key-events": "off",
    "react/prop-types": "off",
    "react/jsx-props-no-spreading": "off",
    "camelcase": "warn",
    "linebreak-style": [2, "windows"]
  }
}